#include <iostream>
#include <cstdio>
using namespace std;
void vk (int a, int b)
{
    for(int i=a;i<=b;i++)
    {
        switch(i){
            case 1:cout<<"one\n";
            break;
             case 2:cout<<"two\n";
            break;
             case 3:cout<<"three\n";
            break;
             case 4:cout<<"four\n";
            break;
             case 5:cout<<"five\n";
            break;
             case 6:cout<<"six\n";
            break;
             case 7:cout<<"seven\n";
            break;
             case 8:cout<<"eight\n";
            break;
             case 9:cout<<"nine\n";
            break;
            default:if(i%2)
           cout<<"odd\n";
           else 
           cout<<"even\n";

           }

        }
    }
    int main(){
 int a ,b;
            cin>>a>>b;
            vk(a
            5
            ,b);
            return 0;
    }
    5
int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */

    int n;
    cin>>n;

    int arr[n];

    for(int i=0; i<=n; i++){
        cin>>arr[i];
    }   

    for(int i= n-1; i>=0; i--){
        cout << arr[i] << " ";
    }
    return 0;
}
